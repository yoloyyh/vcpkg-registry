{
  "name": "aio",
  "version": "1.0.5",
  "description": "C++ asynchronous framework",
  "homepage": "https://github.com/yoloyyh/aio",
  "dependencies": [
    {
      "name": "curl",
      "default-features": false
    },
    "libevent",
    "nlohmann-json",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "zero",
      "version>=": "1.0.4"
    }
  ],
  "default-features": [
    "openssl"
  ],
  "features": {
    "embedded": {
      "description": "Embedding CA certs"
    },
    "openssl": {
      "description": "Support for openssl",
      "dependencies": [
        {
          "name": "curl",
          "default-features": false,
          "features": [
            "openssl"
          ]
        },
        {
          "name": "libevent",
          "features": [
            "openssl"
          ]
        }
      ]
    }
  }
}
